									<li><a href="#"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
								</ul>
							</nav>
							<!-- socials end -->
							<div class="description"><span class="name">Georgina Wilson</span><span class="position">Office Manager</span></div>
						</div>
					</div>
					<!-- item end-->
				</div>
			</div>
		</div>
	</section>
	<!-- team end-->
</main>