									<li><a href="#"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
								</ul>
							</nav>
							<!-- socials end -->
							<div class="description"><span class="name">Carla Santos</span><span class="position">Chief People Officer</span></div>
						</div>
					</div>
					<!-- item end-->
				</div>
				<div class="col-lg-4 col-sm-6">
					<!-- item start-->
					<div class="team-item team-item--primary">
						<div class="img-holder">
							<img class="img-bg" src="~/img/team-bg-5.jpg" alt="img" />
							<!-- socials start-->
