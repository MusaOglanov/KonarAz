						<div class="sliders-holder">
							<!-- product slider start-->
							<div class="product-slider">
								@foreach (ProductImage prodImage in item.ProductImages)
								{
									<div class="slider-item">
										<img src="img/@prodImage.Image" alt="img" />
										<svg class="icon">
											<use xlink:href="#search"></use>
										</svg>
									</div>
								}
							</div>
							<!-- product slider end-->
							<!-- nav slider start-->
							<div class="nav-slider">
								@foreach (ProductImage prodImage in item.ProductImages)
								{
									<div class="slider-item"><img src="img/@prodImage.Image" alt="img" /></div>
								}
							</div>
							<!-- nav slider end-->
						</div>
					</div>

					<div class="col-lg-5 col-md-10 offset-md-1 offset-lg-0">
						<!-- product about start-->
						<div class="product-about">
							<!-- status block start-->
							<div class="status-block">
								@if (item.ProductDetail.HasStock)
								{
									<p>Status: <span class="current-status">Stokda</span></p>

								}
								else
								{
									<p>Status: <span class="text-danger">Stokda yoxdur</span></p>
								}
							</div>
							<!-- status block end-->
							<!-- details start-->
							<div class="details">
								<h4 class="name">@item.Name</h4>
								<span class="price">$ @item.Price</span>
								<div class="details-inner">
									<p>Matreal: <span class="value">@item.ProductDetail.Material</span></p>
									<p>Brend: <span class="value">@item.Brand.Name</span></p>
								</div>
							</div>
							<!-- details end-->
							<!-- tags block start-->
							<div class="tags-block">
								<span class="name">Tags:</span>
								@foreach (ProductTag prodTag in item.ProductTags)
								{
									<span class="tag">
										@prodTag.Tag.Name
										<span class="close">
											<svg class="icon">
												<use xlink:href="#close"></use>
											</svg>
										</span>
									</span>
								}
