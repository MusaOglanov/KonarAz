@model Bio

<div class="card">
    <div class="card-body">
        <h4 class="card-title">Sual yarat</h4>
        <form method="post" class="forms-sample" enctype="multipart/form-data">
            <div class="form-group">
                <label asp-for="Adres">Adres</label>
                <input asp-for="Adres" type="text" class="form-control" />
                <span class="text-danger" asp-validation-for="Adres"></span>
            </div>
           <div class="form-group">
                <label asp-for="Tel">Telefon</label>
                <input asp-for="Tel" type="text" class="form-control" />
                <span class="text-danger" asp-validation-for="Tel"></span>
            </div>
           <div class="form-group">
                <label asp-for="Mail">Email</label>
                <input asp-for="Mail" type="text" class="form-control" />
                <span class="text-danger" asp-validation-for="Mail"></span>
            </div>
           <div class="form-group">
                <label asp-for="Facebook"></label>
                <input asp-for="Facebook" type="text" class="form-control" />
                <span class="text-danger" asp-validation-for="Facebook"></span>
            </div>
           <div class="form-group">
                <label asp-for="Instagram"></label>
                <input asp-for="Instagram" type="text" class="form-control" />
                <span class="text-danger" asp-validation-for="Instagram"></span>
            </div>
           <div class="form-group">
                <label asp-for="Linkedin"></label>
                <input asp-for="Linkedin" type="text" class="form-control" />
                <span class="text-danger" asp-validation-for="Linkedin"></span>
            </div>
           <div class="form-group">
                <label asp-for="Whatsapp"></label>
                <input asp-for="Whatsapp" type="text" class="form-control" />
                <span class="text-danger" asp-validation-for="Whatsapp"></span>
            </div>
         
           
            <button type="submit" class="btn btn-primary me-2">Send</button>
            <a asp-action="Index" class="btn btn-secondary text-white">Go Back</a>
        </form>
    </div>
</div>
